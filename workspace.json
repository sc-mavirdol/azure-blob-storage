{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "document-management",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint",
        "strict": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "document-management": {
      "root": "apps/document-management",
      "sourceRoot": "apps/document-management/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/document-management",
            "index": "apps/document-management/src/index.html",
            "main": "apps/document-management/src/main.tsx",
            "polyfills": "apps/document-management/src/polyfills.ts",
            "tsConfig": "apps/document-management/tsconfig.app.json",
            "assets": [
              "apps/document-management/src/favicon.ico",
              "apps/document-management/src/assets"
            ],
            "styles": ["apps/document-management/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/document-management/src/environments/environment.ts",
                  "with": "apps/document-management/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "document-management:build",
            "hmr": true,
            "ssl": true,
            "sslKey": "certificates/localhost.key",
            "sslCert": "certificates/localhost.crt"
          },
          "configurations": {
            "production": {
              "buildTarget": "document-management:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/document-management/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/document-management"],
          "options": {
            "jestConfig": "apps/document-management/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "document-management-e2e": {
      "root": "apps/document-management-e2e",
      "sourceRoot": "apps/document-management-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/document-management-e2e/cypress.json",
            "tsConfig": "apps/document-management-e2e/tsconfig.e2e.json",
            "devServerTarget": "document-management:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "document-management:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/document-management-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "document-management-nextjs": {
      "root": "apps/document-management-nextjs",
      "sourceRoot": "apps/document-management-nextjs",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/document-management-nextjs",
            "outputPath": "dist/apps/document-management-nextjs"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "document-management-nextjs:build",
            "dev": true,
            "customServerPath": "./server.js"
          },
          "configurations": {
            "production": {
              "buildTarget": "document-management-nextjs:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "document-management-nextjs:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/document-management-nextjs"],
          "options": {
            "jestConfig": "apps/document-management-nextjs/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/document-management-nextjs/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "document-management-nextjs-e2e": {
      "root": "apps/document-management-nextjs-e2e",
      "sourceRoot": "apps/document-management-nextjs-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/document-management-nextjs-e2e/cypress.json",
            "tsConfig": "apps/document-management-nextjs-e2e/tsconfig.e2e.json",
            "devServerTarget": "document-management-nextjs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "document-management-nextjs:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/document-management-nextjs-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}
